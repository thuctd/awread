{
  "$schema": "http://json-schema.org/schema",
  "id": "ui-page",
  "type": "object",
  "properties": {
    "list": {
      "type": "string",
      "description": "ui page name, like ui, example: register,login",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "project": {
      "type": "string",
      "description": "project name"
    },
    "kind": {
      "type": "string",
      "description": "ui or feature kind",
      "default": "page"
    },
    "type": {
      "type": "string",
      "description": "type of component like page, layout v.v..",
      "default": "page"
    }
  },
  "required": [
    "list"
  ]
}