{
  "$schema": "http://json-schema.org/schema",
  "id": "page",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Page name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "project": {
      "type": "string",
      "description": "Project name to add the page to"
    },
    "directory": {
      "type": "string",
      "description": "directory name to add the page to"
    },
    "featureName": {
      "type": "string",
      "description": "feature name, writer-web-feature-auth, have name is auth"
    }
  },
  "required": [
    "name",
    "project",
    "directory",
    "featureName"
  ]
}
