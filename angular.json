          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/writer/mobile/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "writer-mobile-e2e": {
      "root": "apps/writer/mobile-e2e",
      "sourceRoot": "apps/writer/mobile-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/writer/mobile-e2e/cypress.json",
            "tsConfig": "apps/writer/mobile-e2e/tsconfig.e2e.json",
            "devServerTarget": "writer-mobile:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "writer-mobile:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/writer/mobile-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "fcd2e8ed-f162-4e5f-b24a-dbee26739300"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "strict": true
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "api"
}
